<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint
        name="TinderServer"
        address="net.tcp://localhost:40000/Server"
        binding="netTcpBinding"
        contract="Interface.IServer"
        bindingConfiguration="InsecureTcp" />
    </client>

    <bindings>
      <netTcpBinding>
        <binding name="InsecureTcp" closeTimeout="00:00:05" openTimeout="00:00:05" receiveTimeout="00:00:05"
                 sendTimeout="00:00:05" transferMode="Buffered" transactionProtocol="OleTransactions"
                 maxBufferPoolSize="2147483647" maxBufferSize="2147483647"
                 maxConnections="10" maxReceivedMessageSize="2147483647">
          <security mode="Transport" />
          <readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647"
                        maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>

</configuration>
